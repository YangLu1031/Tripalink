<script>
  $(function() {
      $(window).scroll(function(){
          var scrollTop = $(window).scrollTop();
          if(scrollTop != 0) {
            $('.menu-opener-inner').addClass('menu-opener-inner_white');
          }
          else {
            $('.menu-opener-inner').removeClass('menu-opener-inner_white'); 
          }
      });
  });
</script>


