<div class="container">
  <div class="row">
    <div class="col-md-12">
      <hr class="tall">
    </div>

    <% unless @unfinished_reports.empty? %>
      <div class="col-md-12">
        <h4>Unfinished Reports</h4>
      </div>
      <%= render partial: "shared/report", locals: { reports: @unfinished_reports } %>
    <% end %>
  </div>

  <% unless @finished_reports.empty? %>
    <div class="col-md-12">
      <h4>Finished Reports</h4>
    </div>
    <%= render partial: "shared/report", locals: { reports: @finished_reports } %>
  <% end %>
</div>
</div>
